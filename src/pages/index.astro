---
import Layout from '@layouts/Layout.astro'
import Landing from '@components/sections/Landing.astro'
import CodigoDeConducta from '@components/sections/CodigoDeConducta.astro'
import EducationAndTraining from '@components/sections/EducationAndTraining.astro'
import Agenda from '@components/sections/Agenda.astro'
import Experience from '@components/sections/Experience.astro'
import Contacto from '@components/sections/Contacto.astro'
const ticketUserName = Astro.url.searchParams.get('ticket') ?? '';
---

<Layout title='Cesar Napoles - Experienced Web developer' ticketUserName={ticketUserName}>
	<Landing />
	<Experience />
	<EducationAndTraining />
	<Agenda />	
	<Contacto />
</Layout>

